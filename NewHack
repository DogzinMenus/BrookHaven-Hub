-- DataBreakerZ - Loop para excluir 'RequestCooldowns' localmente
-- Criado por DogzinGhost_YT

-- Serviços
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Loop de verificação constante
task.spawn(function()
    while true do
        for _, obj in ipairs(game:GetDescendants()) do
            if obj:IsA("RemoteEvent") and obj.Name == "RequestCooldowns" then
                pcall(function()
                    obj:Destroy()
                    warn("RemoteEvent 'RequestCooldowns' foi excluído localmente.")
                end)
            end
        end
        task.wait(1) -- Espera 1 segundo antes de verificar de novo
    end
end)
